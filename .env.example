# Server Configuration
NODE_ENV=production
PORT=3000
LOG_LEVEL=info

# Google Sheets Configuration
GOOGLE_SHEETS_ID=your-google-sheet-id
GOOGLE_SHEETS_RANGE=Sheet1!A1:M1000

# GCP Configuration
GCP_PROJECT_ID=your-gcp-project-id
GCP_SERVICE_ACCOUNT_EMAIL=your-service-account@your-project.iam.gserviceaccount.com

# Meta (Facebook/Instagram) Configuration
META_APP_ID=your-meta-app-id
META_APP_SECRET=your-meta-app-secret
META_PAGE_ID=your-page-id
META_IG_BUSINESS_ACCOUNT_ID=your-ig-business-account-id
META_PAGE_ACCESS_TOKEN=your-page-access-token

# X (Twitter) Configuration
X_API_KEY=your-x-api-key
X_API_SECRET=your-x-api-secret
X_ACCESS_TOKEN=your-x-access-token
X_ACCESS_TOKEN_SECRET=your-x-access-token-secret
X_BEARER_TOKEN=your-x-bearer-token

# Orchestrator Configuration
ORCHESTRATOR_SECRET=your-secure-secret
FIRESTORE_ENABLED=false
